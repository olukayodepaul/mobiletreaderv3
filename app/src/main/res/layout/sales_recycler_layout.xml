<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
android:layout_width="match_parent"
android:layout_height="wrap_content"
    android:paddingRight="8dip"
    android:paddingLeft="0dip"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/cbg"
    android:background="@drawable/underline_constrain"
    android:paddingBottom="1dp"
    android:layout_marginBottom="1dp"
    android:elevation="6dp"
    >

    <TextView
        android:id="@+id/textView4q"
        android:layout_width="0dip"
        android:layout_height="40dp"
        android:layout_margin="2dip"
        android:ellipsize="end"
        android:gravity="fill_vertical"
        android:singleLine="true"
        android:text="Big TextView"
        android:textSize="14sp"
        app:layout_constraintEnd_toStartOf="@+id/textView6q"
        app:layout_constraintHorizontal_chainStyle="spread_inside"
        app:layout_constraintHorizontal_weight="6.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="HardCodedText"
        android:fontFamily="@font/roboto_light"
        />

    <TextView
        android:text="SOQ"
        android:layout_width="0dip"
        android:layout_height="40dp"
        android:id="@+id/textView6q"
        android:singleLine="true"
        android:ellipsize="end"
        android:gravity="center"
        android:layout_margin="2dip"
        android:textSize="12sp"
        app:layout_constraintHorizontal_weight="2"
        app:layout_constraintStart_toEndOf="@+id/textView4q"
        app:layout_constraintEnd_toStartOf="@+id/editTextq"
        app:layout_constraintTop_toTopOf="@+id/textView4q"
        app:layout_constraintBottom_toBottomOf="@+id/textView4q"
        tools:ignore="HardCodedText"
        android:focusable="true"
        android:focusableInTouchMode="true"/>
    <EditText
        android:layout_width="0dip"
        android:layout_height="40dip"
        android:inputType="numberDecimal"
        android:singleLine="true"
        android:ems="10"
        android:textSize="14sp"
        android:id="@+id/editTextq"
        android:layout_margin="2dip"
        app:layout_constraintHorizontal_weight="2"
        app:layout_constraintStart_toEndOf="@+id/textView6q"
        app:layout_constraintEnd_toStartOf="@+id/editText2q"
        app:layout_constraintBottom_toBottomOf="@+id/textView6q"
        app:layout_constraintTop_toTopOf="@+id/textView6q"
        tools:ignore="HardCodedText,AutoFill"
        android:background="@drawable/edittext"
        android:padding="5dp"
        android:hint="0.0"
        android:focusable="true"
        android:focusableInTouchMode="true"
        />
    <EditText
        android:layout_width="0dip"
        android:layout_height="40dip"
        android:inputType="number"
        android:singleLine="true"
        android:ems="10"
        android:textSize="14sp"
        android:id="@+id/editText2q"
        android:layout_margin="2dip"
        app:layout_constraintHorizontal_weight="2"
        app:layout_constraintStart_toEndOf="@+id/editTextq"
        app:layout_constraintEnd_toStartOf="@+id/editText3q"
        app:layout_constraintBottom_toBottomOf="@+id/editTextq"
        app:layout_constraintTop_toTopOf="@+id/editTextq"
        tools:ignore="HardCodedText,AutoFill"
        android:background="@drawable/edittext"
        android:padding="5dp"
        android:hint="0"
        android:focusable="true"
        android:focusableInTouchMode="true"/>
    <EditText
        android:layout_width="0dip"
        android:layout_height="40dip"
        android:inputType="numberDecimal"
        android:ems="10"
        android:textSize="14sp"
        android:singleLine="true"
        android:id="@+id/editText3q"
        android:layout_margin="2dip"
        app:layout_constraintHorizontal_weight="2"
        app:layout_constraintStart_toEndOf="@+id/editText2q"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="@+id/editText2q"
        app:layout_constraintTop_toTopOf="@+id/editText2q"
        tools:ignore="HardCodedText,AutoFill"
        android:background="@drawable/edittext"
        android:padding="5dp"
        android:hint="0.0"
        android:focusable="true"
        android:focusableInTouchMode="true"/>

</android.support.constraint.ConstraintLayout>


